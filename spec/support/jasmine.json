{
  "spec_dir": "dist",
  "spec_files": [
    "**/tests/*[sS]pec.?(m)js"
  ],
  "helpers": [
    "tests/helpers/**/*.?(m)js"
  ],
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "displayStacktrace": "all"
      }
    }
  ],
  "env": {
    "stopSpecOnExpectationFailure": false,
    "random": false
  }
}
